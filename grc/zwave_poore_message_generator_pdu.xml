<?xml version="1.0"?>
<block>
  <name>message_generator_pdu</name>
  <key>zwave_poore_message_generator_pdu</key>
  <category>[zwave_poore]</category>
  <import>import zwave_poore</import>
  <make>zwave_poore.message_generator_pdu($repetition_interval, $configuration, $home_id, $source_node_id, $frame_control, $destination_node_id, $command_class, $command)</make>
  
  <param>
    <name>Repetition Interval</name>
    <key>repetition_interval</key>
    <value>1</value>
    <type>float</type>
  </param>  

  <param>
    <name>Configuration</name>
    <key>configuration</key>
    <value>1</value>
    <type>float</type>
  </param>    
  
  <param>
    <name>Home ID</name>
    <key>home_id</key>
    <value>"fa1c0b48"</value>
    <type>string</type>
  </param>  
   
  <param>
    <name>Source Node ID</name>
    <key>source_node_id</key>
    <value>"01"</value>
    <type>string</type>
  </param>
  
  <param>
    <name>Frame Control</name>
    <key>frame_control</key>
    <value>"4107"</value>
    <type>string</type>
  </param>
  
  <param>
    <name>Destination Node ID</name>
    <key>destination_node_id</key>
    <value>"02"</value>
    <type>string</type>
  </param>       

  <param>
    <name>Command Class</name>
    <key>command_class</key>
    <value>"26"</value>
    <type>string</type>
  </param>  
  
  <param>
    <name>Command</name>
    <key>command</key>
    <value>"0163"</value>
    <type>string</type>
  </param>    
  
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
